package ticketing;

public class Passenger {
	
	private int id;
	private String name;
	private String source;
	private String destination;
	private int km;
	
	
	
	public Passenger() {
		
	}



	public Passenger(int id, String name, String source, String destination, int km) {
		
		this.id = id;
		this.name = name;
		this.source = source;
		this.destination = destination;
		this.km = km;
	}



	public int getId() {
		return id;
	}



	public void setId(int id) {
		this.id = id;
	}



	public String getName() {
		return name;
	}



	public void setName(String name) {
		this.name = name;
	}



	public String getSource() {
		return source;
	}



	public void setSource(String source) {
		this.source = source;
	}



	public String getDestination() {
		return destination;
	}



	public void setDestination(String destination) {
		this.destination = destination;
	}



	public int getKm() {
		return km;
	}



	public void setKm(int km) {
		this.km = km;
	}
	
	
	
	

}
